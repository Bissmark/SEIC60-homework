<h2>Units Completed</h2>

<div class="unit-container">
    <% @units.each do |unit| %>
        <p>
            <a href="/units/<%= unit["id"] %>">
                <%= unit["code"] %>
            </a>
        </p>
    <% end %>
</div>

<div class="button-container">
    <a href="/units/add">
        <button>Add Unit</button>
    </a>
</div>

<a href="/">
    <button class="back">Back</button>
</a>