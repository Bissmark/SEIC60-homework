<%= form_for @director do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :nationality %>
    <%= f.text_field :nationality %>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
    <%= f.label :movies %>
    <%= f.text_field :movies %>
    <%= f.label :image %>
    <%= f.text_field :image %>

    <%= f.submit %>
<% end %>